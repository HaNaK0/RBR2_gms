            <string>draw_sprite(mySprite, currentFrame, x,y)

//debug
//Only for bugtesting
if global.debug
{
    draw_set_colour(c_lime)
    if attack != dir.no
    {
        draw_line(x + swordHiltPos[X],y + swordHiltPos[Y],x + swordTipPos[X],y + swordTipPos[Y])
    }
    
    draw_rectangle(x - 10, y - 11, x + 10, y + 11, true)
}


