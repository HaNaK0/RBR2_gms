            <string>//setting up the variables to keep wher the new object will end up
var tempPosX = x
var tempPosY = y

//goal collison
if distance_to_object(obj_goal) &lt; 32
{
    if collision_rectangle(x - 10, y - 11, x + 10, y - 11, obj_goal, false, true) != noone
    {
        instance_create(x,y, obj_loading)
    }
}

//close the game
if keyboard_check(vk_escape)
{
    game_end()
}

// fall check
//checks if bob is standing on a tile and sets isFalling to false if he is
if  collision_line(x - 10, y + 16, x + 10, y + 16, obj_tile, false, false) != noone
{
    isFalling = false
}
else
{
    isFalling = true
}


//wall check
//checks if bob is next to a wall
if collision_line(x - 11, y + 11, x - 11, y - 12, obj_tile, false, false) &amp;&amp; keyboard_check(ord('A'))
{
    closeToWall = wall.LEFT
}
else if collision_line(x + 11, y + 11, x + 11, y - 12, obj_tile, false, false) &amp;&amp; keyboard_check(ord('D'))
{
    closeToWall = wall.RIGHT
}
else if collision_line(x + 11, y + 11, x + 11, y - 12, obj_tile, false, false) == noone
{
    closeToWall = wall.NO
}

//wall grab
if closeToWall &amp;&amp; (( keyboard_check(ord('A')) || keyboard_check(ord('D')) ) ||  wallRelDel == true)
{   
    fallLimit = wallGlide
}
else
{
    fallLimit = maxFallsp
}

if (keyboard_check_released(ord('A')) || keyboard_check_released(ord('D')))
{
    wallRelDel = true
    relDelTimer = releaseDelay * fps
}

//timer update
if relDelTimer &gt;= 0
{
    relDelTimer -=1
}

if relDelTimer == 0
{
    wallRelDel = false
}

/*
timers:
[0] attack right
[1] attack up
[2] attack left
[3] attack down
[4] combo-space
[5] main
[6] attack duartion
*/
for (i = 0; i  &lt; nrOfTimers; i++)
{
    if cooldownTimers[i] &gt;= 0
    {
        cooldownTimers[i] -= 1
    }
}

//if bob is falling then accellerate downwards
if isFalling == true
{
    fallSpeed += grav
    if fallSpeed &gt; fallLimit
    {
        fallSpeed = fallLimit;
    }    
}
else
{
    fallSpeed = 0
}

//jump input
if keyboard_check_pressed(vk_space) == true &amp;&amp; isFalling == false
{
    fallSpeed = jumpForce
}
else if keyboard_check_pressed(vk_space) &amp;&amp; closeToWall = wall.LEFT
{
    fallSpeed = jumpForce
    hSpeed = wallJump
}
else if keyboard_check_pressed(vk_space) &amp;&amp; closeToWall = wall.RIGHT
{
    fallSpeed = jumpForce
    hSpeed = -wallJump
}

// attack lunge 
if cooldownTimers[6] &gt; 0
{
    switch attack
    {
    case dir.right:
        hSpeed = lungeSpeed
        break
    case dir.up:
        fallSpeed = -lungeSpeed
        break
    case dir.left:
        hSpeed = -lungeSpeed
        break
    case dir.down:
        fallSpeed = lungeSpeed
        break
    }
}

//input left and right
if keyboard_check(ord('A'))
{
    mySprite = spr_bob_left
    if hSpeed &gt; -mySpeed
    { 

        if isFalling
        {
            hSpeed -= accInAir
        }
        else
        {
            hSpeed -= accelleration
        }
    }
}
else if keyboard_check(ord('D'))
{
    mySprite = spr_bob_right
    if hSpeed &lt; mySpeed
    {
        if isFalling
        {
            hSpeed += accInAir
        }
        else
        {
            hSpeed += accelleration
        }
    }  
}
else
{
    if hSpeed &gt; 0
    {
        if isFalling
        {
            hSpeed -= dragInAir
        }
        else
        {
            hSpeed -= drag
        }
        
        if hSpeed &lt; 0
        {
            hSpeed = 0
        }
    }
    else if hSpeed &lt; 0
    {
        if isFalling
        {
            hSpeed += dragInAir
        }
        else
        {
            hSpeed += drag
        }
        
        if hSpeed &gt; 0
        {
            hSpeed = 0
        }
    }
}

// bob animation state runing or idle
if isFalling
{
    
}
if hSpeed != 0
{
    bobState = state.runing
}
else
{
    bobState = state.idle_1
}

//animation
animUpdateTim += 1
if animUpdateTim &gt;= fps / animSp
{
    animUpdateTim = 0
    switch bobState
    {
    case state.runing:
        currentFrame += 1
        if currentFrame &gt; 12 || currentFrame &lt; 7
        {
            currentFrame = 7
        }
        break
    case state.idle_1:
        currentFrame += 1
        if currentFrame &gt; 5 || currentFrame &lt; 3
        {
            currentFrame = 3
        }
        break
    }
}

//attack
//attacking right
if keyboard_check_pressed(vk_right) &amp;&amp; cooldownTimers[timers.attackR] &lt;= 0 &amp;&amp; !swordLock[dir.right]
{
    if attack ==  dir.no
    {
        attack = dir.right
        cooldownTimers[6] = attackDuration * fps
        cooldownTimers[timers.attackR] = cooldownMain * fps
        hold[dir.right] = true
        swordLock[dir.right] = true
    }
    else
    {
        switch attack
        {
        case dir.up:
        case dir.down:
            followUp = dir.right
            cooldownTimers[timers.attackR] = cooldownMain * fps
            hold[dir.right] = true
            swordLock[dir.right] = true
            break;
        }
    }
}
//attacking upwards
else if keyboard_check_pressed(vk_up) &amp;&amp; cooldownTimers[timers.attackU] &lt;= 0 &amp;&amp; !swordLock[dir.up] &amp;&amp; isFalling
{
    if attack == dir.no
    {
        attack = dir.up
        cooldownTimers[6] = attackDuration * fps
        cooldownTimers[7] = attackHold * fps
        cooldownTimers[timers.attackU] = cooldownMain * fps
        hold[dir.up] = true
        swordLock[dir.up] = true
    }
    else
    {
        switch attack
        {
        case dir.left:
        case dir.right:
            followUp = dir.up
            cooldownTimers[timers.attackU] = cooldownMain * fps
            hold[dir.up] = true
            swordLock[dir.up] = true
            break
        }
    }
}
//attacking left
else if keyboard_check_pressed(vk_left) &amp;&amp; cooldownTimers[timers.attackL] &lt;= 0 &amp;&amp; !swordLock[dir.left]
{
    if attack == dir.no
    {
        attack = dir.left
        cooldownTimers[6] = attackDuration * fps
        cooldownTimers[7] = attackHold * fps
        cooldownTimers[timers.attackL] = cooldownMain * fps
        hold[dir.left] = true
        swordLock[dir.left] = true
    }
    else
    {
        switch attack
        {
        case dir.up:
        case dir.down:
            followUp = dir.left
            cooldownTimers[timers.attackL] = cooldownMain * fps
            hold[dir.left] = true
            swordLock[dir.left] = true
            break;
        }
    }
}
//attacking left
else if keyboard_check_pressed(vk_down) &amp;&amp; cooldownTimers[timers.attackD] &lt;= 0 &amp;&amp; isFalling &amp;&amp; swordLock[dir.down]
{
    if attack == dir.no
    {
        attack = dir.down
        cooldownTimers[6] = attackDuration * fps
        cooldownTimers[7] = attackHold * fps
        cooldownTimers[timers.attackD] = cooldownMain * fps
        hold[dir.down] = true
        swordLock[dir.down] = true
    }
    else
    {
        switch attack
        {
        case dir.left:
        case dir.right:
            followUp = dir.down
            cooldownTimers[timers.attackD] = cooldownMain * fps
            hold[dir.down] = true
            swordLock[dir.down] = true
            break;
        }
    }
}



//attack keys released
if keyboard_check_released(vk_right)//right
{
    hold[dir.right] = false
    if cooldownTimers[6] &lt;= 0 &amp;&amp; comboing == false
    {
        attack = dir.no 
    }
}

if keyboard_check_released(vk_up)//up
{
    hold[dir.up] = false
    if cooldownTimers[6] &lt;= 0 &amp;&amp; comboing == false
    {
        attack = dir.no 
    }
}

if keyboard_check_released(vk_left)// left
{
    hold[dir.left] = false
    if cooldownTimers[6] &lt;= 0 &amp;&amp; comboing == false
    {
        attack = dir.no 
    }
}

if keyboard_check_released(vk_down) //down
{
    hold[dir.down] = false
    if cooldownTimers[6] &lt;= 0 &amp;&amp; comboing == false
    {
        attack = dir.no 
    }
}

//attack animation and collision
if attack != dir.no &amp;&amp; comboing == false
{
    var hitEnemy = collision_line(x + swordHiltPos[X],y + swordHiltPos[Y],x + swordTipPos[X],y + swordTipPos[Y], obj_enemy, false, false)
    
    if cooldownTimers[6] &gt;= 0
    {
        switch attack
        {
        case dir.right:
            swordTipPos[X] = sRightEndPos[2] * (1- cooldownTimers[6]/(attackDuration * fps))
            break
        case dir.up:
            swordTipPos[Y] = sUpEndPos[3] * (1- cooldownTimers[6]/(attackDuration * fps))
            break
        case dir.left:
            swordTipPos[X] = sLeftEndPos[2] * (1- cooldownTimers[6]/(attackDuration * fps))
            break
        case dir.down:
            swordTipPos[X] = sDownEndPos[2] * (1- cooldownTimers[6]/(attackDuration * fps))
            swordTipPos[Y] = sDownEndPos[3] * (1- cooldownTimers[6]/(attackDuration * fps))
            break
        }
    }
    else
    {
        switch attack
        {
        case dir.right:
            swordTipPos[X] = sRightEndPos[2] 
            break
        case dir.up:
            swordTipPos[Y] = sUpEndPos[3] 
            break
        case dir.left:
            swordTipPos[X] = sLeftEndPos[2] 
            break
        case dir.down:
            swordTipPos[X] = sDownEndPos[2] 
            swordTipPos[Y] = sDownEndPos[3] 
            break
        }
    }
    
    // enemy gets destoryed and time is gven to player
    if hitEnemy != noone
    {
        with(hitEnemy)
        {
        instance_destroy()
        }
    addTime(enemyTime) //timer ad time script
    }
}
else if attack != dir.no
{
    //this will be messy 
    //but first it checks for which animation to do and then it executes it
    var hitEnemy = collision_line(x + swordHiltPos[X],y + swordHiltPos[Y],x + swordTipPos[X],y + swordTipPos[Y], obj_enemy, false, false)
    
    switch attack
    {
    case dir.right:
        switch prevAttack
        {
        case dir.up:
            //up -&gt; right
            swordTipPos[X] = lengthdir_x(sUpEndPos[3], 90 - 90 * (1- cooldownTimers[6]/(attackDuration * fps)) + 180)
            swordTipPos[Y] = lengthdir_y(sUpEndPos[3], 90 - 90 * (1- cooldownTimers[6]/(attackDuration * fps)) + 180)
            break
        case dir.down:
            //down -&gt; right
            swordTipPos[X] = lengthdir_x(sUpEndPos[3], -90 + 90 * (1- cooldownTimers[6]/(attackDuration * fps)) +180)
            swordTipPos[Y] = lengthdir_y(sUpEndPos[3], -90 + 90 * (1- cooldownTimers[6]/(attackDuration * fps)) +180)
            break
        }
        break
                
    case dir.up:
        switch prevAttack
        {
        case dir.right:
            //rigth -&gt; up
            swordTipPos[X] = lengthdir_x(sUpEndPos[3], 90 * (1- cooldownTimers[6]/(attackDuration * fps)) +180)
            swordTipPos[Y] = lengthdir_y(sUpEndPos[3], 90 * (1- cooldownTimers[6]/(attackDuration * fps)) +180)
            break
        case dir.left:
            //left -&gt; up
            swordTipPos[X] = lengthdir_x(sUpEndPos[3], 180 - 90 * (1- cooldownTimers[6]/(attackDuration * fps)) + 180)
            swordTipPos[Y] = lengthdir_y(sUpEndPos[3], 180 - 90 * (1- cooldownTimers[6]/(attackDuration * fps)) + 180)
            break
        }
        break
        
    case dir.left:
        switch prevAttack
        {
        case dir.up:
            //up -&gt; left
            swordTipPos[X] = lengthdir_x(sUpEndPos[3], 90 + 90 * (1- cooldownTimers[6]/(attackDuration * fps)) + 180)
            swordTipPos[Y] = lengthdir_y(sUpEndPos[3], 90 + 90 * (1- cooldownTimers[6]/(attackDuration * fps)) + 180)
            break
        case dir.down:
            //down -&gt; left
            swordTipPos[X] = lengthdir_x(sUpEndPos[3], 270 - 90 * (1- cooldownTimers[6]/(attackDuration * fps)) + 180)
            swordTipPos[Y] = lengthdir_y(sUpEndPos[3], 270 - 90 * (1- cooldownTimers[6]/(attackDuration * fps)) + 180)
        }
        break
    
    case dir.down:
        switch prevAttack
        {
        case dir.right:
            //right -&gt; down
            swordTipPos[X] = lengthdir_x(sUpEndPos[3], -90 * (1- cooldownTimers[6]/(attackDuration * fps)) + 180)
            swordTipPos[Y] = lengthdir_y(sUpEndPos[3], -90 * (1- cooldownTimers[6]/(attackDuration * fps)) + 180)
            break
        case dir.left:
            //left -&gt; down
            swordTipPos[X] = lengthdir_x(sUpEndPos[3], 180 + 90 * (1- cooldownTimers[6]/(attackDuration * fps)) + 180)
            swordTipPos[Y] = lengthdir_y(sUpEndPos[3], 180 + 90 * (1- cooldownTimers[6]/(attackDuration * fps)) + 180)
        }
        break    
    }
    
    if hitEnemy != noone
    {
        with(hitEnemy)
        {
        instance_destroy()
        }
    addTime(enemyTime)
    }
}

if attack == dir.no
{
    swordTipPos[X] = 0
    swordTipPos[Y] = 0
}

if cooldownTimers[7] == 0
{
    if followUp != dir.no
    {
        prevAttack = attack
        attack = followUp
        comboing = true
        followUp = dir.no 
        cooldownTimers[6] = attackDuration * fps
        cooldownTimers[7] = attackHold * fps
    }
    else
    {
        comboing = false
        switch attack
        {
        case dir.right:
            if hold[dir.right] == false
            {
                attack = dir.no
            }
            
            break;
        case dir.up:
            if hold[dir.up] == false
            {
                attack = dir.no
            }
            
            
            break;
        case dir.left:
            if hold[dir.left] == false
            {
                attack = dir.no
            }
            
            
            break;
        case dir.down:
            if hold[dir.down] == false
            {
                attack = dir.no
            }    
        }
    }
    
}

if collision_rectangle(x - 10, y - 11, x + 10, y + 11 , obj_enemy, true, true)
{
    mySpeed = mySpeedSlowed
    addTime(-timeLoss)
    monsterCol = true
    if hSpeed &gt; mySpeedSlowed
    {
        hSpeed = mySpeedSlowed
    }
    else if hSpeed &lt; -mySpeedSlowed
    {
        hSpeed = -mySpeedSlowed
    }
}
else
{
    mySpeed = mySpeedMax
    monsterCol = false
}

//gravity collision
if fallSpeed &gt;= 0
{
    var vCollision = collision_line(x - 10, y + 16 + fallSpeed, x + 10, y + 16 + fallSpeed, obj_tile, false, false);
    if vCollision == noone
    {
        tempPosY += fallSpeed;
    }
    else if vCollision.halfTile
    {
        switch vCollision.orientation
        {
            case 0: 
                tempPosY = vCollision.y - 1 *(x - vCollision.x) + 32
                break
        }
    }
    else
    {
        tempPosY = vCollision.y -16 
    }
}
else
{
    var vCollision = collision_line(x - 10, y - 16 + fallSpeed, x + 10, y - 16 + fallSpeed, obj_tile, false, false);
    if vCollision == noone
    {
        tempPosY += fallSpeed;
    }
    else
    {
        tempPosY = vCollision.y +16 +32
        fallSpeed = 0;
    }
}

// horizontal collision
if hSpeed &lt; 0
{   
    var vCollider = collision_line(x -10 + hSpeed, tempPosY - 15, x -10 + hSpeed, tempPosY + 15, obj_tile_full, false, false)
    if vCollider == noone
    {
        tempPosX += hSpeed
    }
    else
    {
        tempPosX = vCollider.x +32 +10
        hSpeed = 0
    }
}
else if hSpeed &gt; 0
{   
    var vCollider = collision_line(x +10 + hSpeed, tempPosY - 15, x +10 + hSpeed, tempPosY + 15, obj_tile_full, false, false);
    if vCollider == noone
    {
        tempPosX += hSpeed
    }
    else
    {
        tempPosX = vCollider.x - 11
        hSpeed = 0
    }
}


// reseting sword
if !isFalling
{
    for(i = 1; i &lt; 5; i++)
    {
         swordLock[i] = false
    } 
}

//movement execution
//all movement is executed here
x = tempPosX
y = tempPosY
</string>
